{
  "name": "e_commerce_project",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist/",
    "build": "npm run clean && tsc --project tsconfig.json",
    "start": "node dist/server.js",
    "start:prod": "npm run build && node --env-file=.env.prod scripts/checkEnv.js && node dist/server.js",
    "start:dev": "nodemon src/server.ts",
    "start:trace": "npm run build && node --trace-sync-io dist/server.js",
    "prebuild": "npm run lint && npm run test",
    "prestart:prod": "npm run build",
    "pretest": "npm run build",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jasmine",
    "test:unit:ts": "npm run build && jasmine",
    "test:integration": "npm run build && jasmine --config integration-test-config.json",
    "test:watch": "nodemon --exec jasmine",
    "format": "prettier --config ./.prettierrc --write \"src/**/*.{ts,js}\"",
    "lint": "eslint --config ./eslint.config.mjs \"src/**/*.{ts,js}\"",
    "lint:fix": "eslint --fix --config ./eslint.config.mjs \"src/**/*.{ts,js}\"",
    "seed:generate": "sequelize-cli seed:generate --name",
    "seed:all": "sequelize-cli db:seed:all",
    "seed:undo": "sequelize-cli db:seed:undo",
    "seed:run": "sequelize-cli db:seed --seed",
    "migrate:generate": "sequelize-cli migration:generate --name",
    "migrate:up": "sequelize-cli db:migrate",
    "migrate:down": "sequelize-cli db:migrate:undo",
    "migrate:up:test": "sequelize-cli db:migrate --env test",
    "migrate:down:test": "sequelize-cli db:migrate:undo --env test",
    "db:reset": "npm run migrate:down && npm run migrate:up && npm run seed",
    "db:reset:test": "npm run migrate:down:test && npm run migrate:up:test && npm run seed"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@faker-js/faker": "^9.4.0",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/config": "^3.3.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/date-fns": "^2.5.3",
    "@types/eslint-plugin-security": "^3.0.0",
    "@types/express": "^5.0.0",
    "@types/hpp": "^0.2.6",
    "@types/jest": "^29.5.14",
    "@types/joi": "^17.2.2",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.2",
    "@types/pg": "^8.11.10",
    "@types/pino": "^7.0.4",
    "@types/pino-http": "^5.8.4",
    "@types/pino-pretty": "^4.7.5",
    "@types/request-ip": "^0.0.41",
    "@types/sequelize": "^4.28.20",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.18.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.18.0",
    "eslint-config-nodesecurity": "^1.3.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-security": "^3.0.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
