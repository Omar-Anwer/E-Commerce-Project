{
  "name": "e-commerce-project",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist/",
    "build": "npm run clean && npx tsc",
    "start": "npm run build && node dist/server.js",
    "start:trace": "npm run build && node --trace-sync-io dist/server.js",
    "test": "npm run build && npm run jasmine",
    "dev": "nodemon src/server.ts",
    "format": "prettier --config ./src/config/.prettierrc --write \"src/**/*.{ts,js}\"",
    "lint": "eslint --config ./src/config/eslint.config.mjs \"src/**/*.{ts,js}\"",
    "lint:fix": "eslint --config ./src/config/eslint.config.mjs \"src/**/*.{ts,js}\" --fix",
    "jasmine": "jasmine",
    "jasmine:ts": "jasmine-ts",
    "migrate:up": "npx sequelize-cli db:migrate",
    "migrate:down": "npx sequelize-cli db:migrate:undo",
    "migrate:up:test": "npx sequelize-cli db:migrate --env test",
    "migrate:down:test": "npx sequelize-cli db:migrate:undo --env test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
